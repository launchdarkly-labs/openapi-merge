{
  "name": "@launchdarkly/openapi-merge",
  "version": "1.3.4",
  "description": "A tool to merge numerous OpenAPI files into a single openapi definition. Forked from https://github.com/robertmassaioli/openapi-merge",
  "keywords": [
    "openapi",
    "merge"
  ],
  "main": "dist/index",
  "typings": "dist/index",
  "files": [
    "dist/!(__tests__)",
    "dist/!(__tests__)/**/*"
  ],
  "author": "LaunchDarkly",
  "license": "MIT",
  "homepage": "https://github.com/launchdarkly-labs/openapi-merge",
  "repository": "github:launchdarkly-labs/openapi-merge",
  "bugs": {
    "url": "https://github.com/launchdarkly-labs/openapi-merge/issues"
  },
  "scripts": {
    "build": "tsc --project .",
    "prepare": "tsc --project .",
    "prepublishOnly": "tsc --project .",
    "test": "jest --collect-coverage --verbose",
    "lint": "eslint src --fix",
    "start": "ts-node --project ./tsconfig.json src/index.ts"
  },
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@babel/preset-env": "^7.9.6",
    "@babel/preset-typescript": "^7.9.0",
    "@eslint/js": "^9.33.0",
    "@types/lodash": "^4.17.0",
    "@typescript-eslint/eslint-plugin": "^8.40.0",
    "@typescript-eslint/parser": "^8.40.0",
    "babel-jest": "^29.7.0",
    "eslint": "^9.33.0",
    "globals": "^15.0.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.0",
    "ts-node": "^8.10.1",
    "typescript": "^5.9.2"
  },
  "dependencies": {
    "atlassian-openapi": "^1.0.8",
    "lodash": "^4.17.15",
    "ts-is-present": "^1.1.1"
  }
}
